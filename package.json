{
  "name": "examples-trpc-next-prisma-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "test-e2e:ci": "TEST_LOCAL=1 pnpm playwright test",
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "dev:nomigrate": "next dev",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache src",
    "lint-fix": "pnpm lint --fix",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test-unit": "vitest",
    "test-e2e": "playwright test",
    "test-start": "run-s test-unit test-e2e",
    "postinstall": "pnpm generate",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@tanstack/react-query": "^5.90.12",
    "@trpc/client": "^11.7.2",
    "@trpc/next": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "clsx": "^2.0.0",
    "next": "^16.0.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "superjson": "^2.2.6",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^22.19.2",
    "@types/react": "^19.2.7",
    "@vitest/ui": "^3.1.2",
    "autoprefixer": "^10.4.22",
    "dotenv": "^16.0.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.10",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "jsdom": "^27.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "prettier": "^3.7.4",
    "prisma": "^7.1.0",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.49.0",
    "vite": "^6.4.1",
    "vitest": "^3.1.2"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
